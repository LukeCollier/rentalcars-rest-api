# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# A summary of the cars
GET     /car/summary                            controllers.CarsController.getCarSummary

# The rating sorted by descending
GET     /car/sort/rating                        controllers.CarsController.getSortCarRating

# Sorted by score ascending
GET     /car/sort/score                         controllers.CarsController.getSortScore

# Price ascending
GET     /car/sort/price                         controllers.CarsController.getSortPrice

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                           controllers.Assets.versioned(path="/public", file: Asset)


